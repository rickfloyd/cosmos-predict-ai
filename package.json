{
  "name": "cosmos-predict-ai",
  "version": "1.0.0",
  "description": "Advanced AI Influencer Assistant with video generation capabilities for veterans and content creators",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:security": "eslint . --ext .js,.jsx,.ts,.tsx --config .eslintrc.js --format json | jq '.[] | select(.ruleId | contains(\"security\"))' || echo 'No security linting issues found'",
    "type-check": "tsc --noEmit",
    "security-check": "node scripts/security-check.js",
    "security-check:ci": "npm run security-check",
    "build": "npm run security-check && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build:dev": "npm run security-check && react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "precommit": "npm run security-check",
    "prepare": "husky install",
    "web": "expo start --web",
    "deploy": "npm run build && echo 'Ready for DigitalOcean deployment'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "expo": "~49.0.15",
    "expo-speech": "^11.3.0",
    "expo-status-bar": "~1.6.0",
    "openai": "^4.14.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.25.0",
    "react-native-vector-icons": "^10.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "ai",
    "influencer",
    "react-native",
    "video-generation",
    "veteran-support",
    "chatbot",
    "content-creation",
    "digitalocean"
  ],
  "author": "Cosmos Predict AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/cosmos-predict-ai.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/cosmos-predict-ai/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/cosmos-predict-ai#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "eslint --config .eslintrc.js --format json | jq '.[] | select(.ruleId | contains(\"security\"))' || echo 'No security issues found'"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
